		"AN_CH4",        "RTC_BATT_FAIL", "I2C_1_CLK",     "I2C_1_DAT",
		"AN_CH0",        "AN_CH1",        "",              "",
		"AN_CH2",        "AN_CH3",        "",              "",
		"",              "",              "",              "",
		"",              "",              "",              "",
		"",              "",              "",              "",
		"",              "",              "",              "",
		"",              "",              "",              "";
};

/* gpiochip4; CPU GPIO2: */
&gpio3 {
	gpio-line-names =
